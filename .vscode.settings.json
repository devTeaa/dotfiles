{
  "color-highlight.markerType": "outline",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.wordWrap": "off",
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorSurroundingLines": 4,
  "editor.detectIndentation": false,
  "editor.fontFamily": "Consolas, 'Courier New', monospace, 'Intel One Mono'",
  "editor.fontSize": 11.5,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.guides.bracketPairs": "active",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.rename.enablePreview": false,
  "editor.rulers": [{}, 80],
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wrappingIndent": "none",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  // "files.associations": {
  //   "*.css": "tailwindcss",
  //   "*.scss": "tailwindcss"
  // },
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "html.autoClosingTags": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredExtensions": ["ms-vscode-remote.remote-wsl-recommender"],
  "terminal.external.windowsExec": "wt -p cmd",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.env.windows": {},
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.useWslProfiles": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "none",
  // "vim.handleKeys": {
  //   "J": false,
  //   "K": false
  // },
  // "vim.normalModeKeyBindingsNonRecursive": [{
  //   "before": ["J"],
  //   "commands": ["editor.action.moveLinesDownAction"]
  // }, // moveLineDown
  // {
  //   "before": ["K"],
  //   "commands": ["editor.action.moveLinesUpAction"] // moveLineUp
  // }],
  "vim.leader": " ",
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "d:\\.vscode.vimrc",
  "window.zoomLevel": 1,
  // "vim.visualModeKeyBindings": [{
  //   "before": ["J"],
  //   "commands": ["editor.action.moveLinesDownAction"]
  // }, // moveLineDown
  // {
  //   "before": ["K"],
  //   "commands": ["editor.action.moveLinesUpAction"] // moveLineUp
  // }],
  // "volar.completion.preferredTagNameCase": "pascal",
  // "volar.inlayHints.eventArgumentInInlineHandlers": false,
  // "volar.inlayHints.missingRequiredProps": false,
  // "vscode-neovim.neovimExecutablePaths.win32": "C:\\Program Files\\Neovim\\bin\\nvim.exe",
  // "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.iconTheme": "material-icon-theme"
}
